{
  "name": "remix-starter-express",
  "version": "1.0.0",
  "scripts": {
    "build": "remix build && tsc -b",
    "dev": "concurrently \"remix run\" \"tsc -b -w\" \"cross-env NODE_ENV=development nodemon -w app -w loaders -e js,ts,tsx server.js\"",
    "start": "node server.js"
  },
  "dependencies": {
    "@remix-run/cli": "^0.7.0",
    "@remix-run/express": "^0.7.0",
    "@remix-run/loader": "^0.7.0",
    "@remix-run/react": "^0.7.0",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-router": "^6.0.0-beta.0",
    "react-router-dom": "^6.0.0-beta.0"
  },
  "devDependencies": {
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "babel-eslint": "^10.1.0",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.14.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "nodemon": "^2.0.5",
    "prettier": "^2.2.0",
    "typescript": "^4.0.3"
  },
  "engines": {
    "node": "12"
  }
}
